{
  "manifest_version": 2,
  "name": "FaceMask",
  "version": "0.0.1",
  "description": "Face? Mask.",
  "short_name": "FaceMask",
  "permissions": [
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://facebook.com/*"
      ],
      "css": [
        "background.css"
      ],
      "js": [
        "background.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}